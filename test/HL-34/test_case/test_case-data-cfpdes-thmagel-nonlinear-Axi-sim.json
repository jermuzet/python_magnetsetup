{
    "Name": "Fully coupled Axi Stationnary model",
    "ShortName": "FullAxiStat",
    "Models": {
        "equations": [
            {
                "name": "heat",
                "unknown": {
                    "basis": "Pch1",
                    "name": "temperature",
                    "symbol": "T"
                }
            },
            {
                "name": "magnetic",
                "unknown": {
                    "basis": "Pch2",
                    "name": "phi",
                    "symbol": "phi"
                }
            },
            {
                "name": "elastic",
                "unknown": {
                    "basis": "Pchv1",
                    "name": "displacement",
                    "symbol": "u"
                }
            }
        ]
    },
    "Parameters": {
        "mu0": "4*pi*1e-7",
        "T0": "293.",
        "Tin": "284.15",
        "bool_laplace": "1",
        "bool_dilatation": "1",
        "Tinit": 293,
        "h": 58222.1,
        "Tw": 290.671,
        "dTw": 12.74,
        "h0": "h:h",
        "Tw0": "Tw:Tw",
        "dTw0": "dTw:dTw",
        "Zmin0": -0.226,
        "Zmax0": 0.108,
        "Sh0": 0.09034717816899689,
        "Dh0": 0.0015200000000000031,
        "h1": "h:h",
        "Tw1": "Tw:Tw",
        "dTw1": "dTw:dTw",
        "Zmin1": -0.226,
        "Zmax1": 0.108,
        "Sh1": 107.80869206238151,
        "Dh1": 0.3339,
        "U_H1_Cu1": "1"
    },
    "Materials": {
        "Air": {
            "physics": "magnetic",
            "magnetic_c": "x/mu0:x:mu0",
            "magnetic_beta": "{2/mu0,0}:mu0"
        },
        "H1_Cu1": {
            "alpha": "0.0036",
            "sigma0": "50100000.0",
            "sigma": "sigma0/(1+alpha*(heat_T-T0)):sigma0:alpha:heat_T:T0",
            "k0": "360.0",
            "k": "k0/(1+alpha*(heat_T-T0))*heat_T/T0:k0:alpha:heat_T:T0",
            "mu": "1.0*mu0:mu0",
            "E": "127000000000.0",
            "nu": "0.335",
            "alphaT": "1.8e-05",
            "U": "U_H1_Cu1:U_H1_Cu1",
            "filename": "$cfgdir/conductor.json"
        }
    },
    "BoundaryConditions": {
        "magnetic": {
            "Dirichlet": {
                "magdir": {
                    "markers": [
                        "ZAxis",
                        "Infty"
                    ],
                    "expr": "0"
                }
            }
        },
        "heat": {
            "Robin": {
                "Channel0": {
                    "expr1": "h0*x:h0:x",
                    "expr2": "x*h0*(Tw0+dTw0):h0:Tw0:dTw0:x"
                },
                "Channel1": {
                    "expr1": "h1*x:h1:x",
                    "expr2": "x*h1*(Tw1+dTw1):h1:Tw1:dTw1:x"
                }
            }
        },
        "elastic": {
            "Dirichlet": {
                "H1_Fixed": {
                    "markers": [
                        "H1_HP",
                        "H_HP"
                    ],
                    "expr": "{0,0}"
                }
            }
        }
    },
    "InitialConditions": {
        "temperature": {
            "Expression": {
                "myic": {
                    "expr": "Tinit:Tinit"
                }
            }
        }
    },
    "PostProcess": {
        "use-model-name": 1,
        "cfpdes": {
            "Exports": {
                "fields": [
                    "heat.temperature",
                    "elastic.displacement"
                ],
                "expr": {
                    "atheta": "magnetic_phi/x:magnetic_phi:x",
                    "B": {
                        "expr": "{-magnetic_grad_phi_1/x,magnetic_grad_phi_0/x}:magnetic_grad_phi_0:magnetic_grad_phi_1:x",
                        "representation": [
                            "element"
                        ]
                    },
                    "U": {
                        "expr": "materials_U:materials_U",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        }
                    },
                    "Jth": {
                        "expr": "-materials_sigma*materials_U/(2*pi*x):materials_sigma:materials_U:x",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        }
                    },
                    "Qth": {
                        "expr": "materials_sigma*(materials_U/(2*pi*x))*(materials_U/(2*pi*x)):materials_sigma:materials_U:x",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        }
                    },
                    "Flaplace": {
                        "expr": "{materials_F_laplace_0,materials_F_laplace_1}:materials_F_laplace_0:materials_F_laplace_1",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        }
                    },
                    "sigma": {
                        "expr": "{materials_sigma_rr,0,materials_sigma_rz, 0,materials_sigma_thth,0, materials_sigma_rz,0,materials_sigma_zz}:materials_sigma_rr:materials_sigma_rz:materials_sigma_thth:materials_sigma_zz",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        },
                        "representation": [
                            "element"
                        ]
                    },
                    "sigma_tr": {
                        "expr": "max(abs(materials_sigma_1-materials_sigma_2),max(abs(materials_sigma_1-materials_sigma_3),abs(materials_sigma_2-materials_sigma_3))):materials_sigma_1:materials_sigma_2:materials_sigma_3",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        },
                        "representation": [
                            "element"
                        ]
                    },
                    "sigma_vm": {
                        "expr": "sqrt(2/3*(materials_sigma_1*materials_sigma_1+materials_sigma_2*materials_sigma_2+materials_sigma_3*materials_sigma_3-materials_sigma_1*materials_sigma_2-materials_sigma_1*materials_sigma_3-materials_sigma_2*materials_sigma_3)):materials_sigma_1:materials_sigma_2:materials_sigma_3",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        },
                        "representation": [
                            "element"
                        ]
                    },
                    "principle_stresses": {
                        "expr": "{materials_sigma_1,materials_sigma_2,materials_sigma_3}:materials_sigma_1:materials_sigma_2:materials_sigma_3",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        },
                        "representation": [
                            "element"
                        ]
                    },
                    "epsilon": {
                        "expr": "{elastic_grad_u_00,0,1/2*(elastic_grad_u_01+elastic_grad_u_10), 0,elastic_u_0/x,0, 1/2*(elastic_grad_u_01+elastic_grad_u_10),0,elastic_grad_u_11}:x:elastic_u_0:elastic_grad_u_00:elastic_grad_u_01:elastic_grad_u_10:elastic_grad_u_11",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        },
                        "representation": [
                            "element"
                        ]
                    }
                }
            }
        },
        "magnetic": {
            "Measures": {
                "Statistics": {
                    "MagneticEnergy": {
                        "type": "integrate",
                        "expr": "-2*pi*magnetic_phi/x*materials_sigma*(materials_U/2/pi):magnetic_phi:materials_sigma:materials_U:x",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        }
                    },
                    "Intensity_H%1_1%_Cu%1_2%": {
                        "type": "integrate",
                        "expr": "-materials_sigma*materials_U/2/pi/x:materials_sigma:materials_U:x",
                        "markers": "H%1_1%_Cu%1_2%",
                        "index1": [
                            [
                                "1",
                                "1"
                            ]
                        ]
                    },
                    "Power_H%1_1%": {
                        "type": "integrate",
                        "expr": "2*pi*materials_sigma*(materials_U/2/pi)*(materials_U/2/pi)/x:materials_sigma:materials_U:x",
                        "markers": {
                            "name": [
                                "H%1_1%_Cu%1_2%"
                            ],
                            "index1": [
                                [
                                    "1",
                                    "1"
                                ]
                            ]
                        }
                    }
                }
            }
        },
        "heat": {
            "Save": {
                "Fields": {
                    "names": [
                        "temperature"
                    ]
                }
            },
            "Measures": {
                "Statistics": {
                    "MeanT": {
                        "type": [
                            "min",
                            "max",
                            "mean"
                        ],
                        "field": "temperature"
                    },
                    "MeanT_H%1_1%_Cu%1_2%": {
                        "type": [
                            "min",
                            "max",
                            "mean"
                        ],
                        "field": "temperature",
                        "markers": "H%1_1%_Cu%1_2%",
                        "index1": [
                            [
                                "1",
                                "1"
                            ]
                        ]
                    },
                    "Flux_Channel%1%": {
                        "type": "integrate",
                        "expr": "h%1%*(heat_T-(Tw%1%+dTw%1%))*2*pi*x:x:heat_T:h%1%:Tw%1%:dTw%1%",
                        "markers": "Channel%1%",
                        "index1": "0:1"
                    },
                    "MeanT_H1": {
                        "type": [
                            "min",
                            "max",
                            "mean"
                        ],
                        "field": "temperature",
                        "markers": {
                            "name": "H1_Cu%1%",
                            "index1": [
                                "1"
                            ]
                        }
                    }
                }
            }
        }
    }
}